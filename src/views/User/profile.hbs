{{#each profile}}
<div class="profile-container">
    <div class="profile-header-bg">
        <div class="header-middle-gradient"></div>
        {{#if this.banner}}
        <img decoding="async" src="https://animegenzai.xyz/public/upload/user/banners/thumb-{{this.banner}}">
        {{/if}}
    </div>
    <div class="profile-header">

        <div class="profile-header-avatar">
            {{#if this.avatar}}
            <img loading="edge" src="https://animegenzai.xyz/public/upload/user/{{this.avatar}}" />
            {{/if}}
        </div>
        <div class="profile-header-name">
            <p>{{this.name}}</p>
        </div>
        <div class="profile-header-social">

            {{#each ../social as |t|}}
            {{#if t.social.instagram}}
            <a href="https://www.instagram.com/{{t.social.instagram}}?gencookie=1" target="_blank">
                <div class="profile-header-social-item" title="instagram">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                        <path
                            d="M12 2.163c3.204 0 3.584.012 4.85.07 3.252.148 4.771 1.691 4.919 4.919.058 1.265.069 1.645.069 4.849 0 3.205-.012 3.584-.069 4.849-.149 3.225-1.664 4.771-4.919 4.919-1.266.058-1.644.07-4.85.07-3.204 0-3.584-.012-4.849-.07-3.26-.149-4.771-1.699-4.919-4.92-.058-1.265-.07-1.644-.07-4.849 0-3.204.013-3.583.07-4.849.149-3.227 1.664-4.771 4.919-4.919 1.266-.057 1.645-.069 4.849-.069zm0-2.163c-3.259 0-3.667.014-4.947.072-4.358.2-6.78 2.618-6.98 6.98-.059 1.281-.073 1.689-.073 4.948 0 3.259.014 3.668.072 4.948.2 4.358 2.618 6.78 6.98 6.98 1.281.058 1.689.072 4.948.072 3.259 0 3.668-.014 4.948-.072 4.354-.2 6.782-2.618 6.979-6.98.059-1.28.073-1.689.073-4.948 0-3.259-.014-3.667-.072-4.947-.196-4.354-2.617-6.78-6.979-6.98-1.281-.059-1.69-.073-4.949-.073zm0 5.838c-3.403 0-6.162 2.759-6.162 6.162s2.759 6.163 6.162 6.163 6.162-2.759 6.162-6.163c0-3.403-2.759-6.162-6.162-6.162zm0 10.162c-2.209 0-4-1.79-4-4 0-2.209 1.791-4 4-4s4 1.791 4 4c0 2.21-1.791 4-4 4zm6.406-11.845c-.796 0-1.441.645-1.441 1.44s.645 1.44 1.441 1.44c.795 0 1.439-.645 1.439-1.44s-.644-1.44-1.439-1.44z" />
                    </svg>
                </div>
            </a>
            {{/if}}
            {{#if t.social.facebook}}
            <a href="https://www.facebook.com/{{t.social.facebook}}?gencookie=1" target="_blank">
                <div class="profile-header-social-item" title="Facebook">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                        <path
                            d="M9 8h-3v4h3v12h5v-12h3.642l.358-4h-4v-1.667c0-.955.192-1.333 1.115-1.333h2.885v-5h-3.808c-3.596 0-5.192 1.583-5.192 4.615v3.385z" />
                    </svg>
                </div>
            </a>
            {{/if}}
            {{#if t.social.youtube}}
            <a href="https://www.youtube.com/c/{{t.social.youtube}}?gencookie=1" target="_blank">
                <div class="profile-header-social-item" title="Youtube">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                        <path
                            d="M19.615 3.184c-3.604-.246-11.631-.245-15.23 0-3.897.266-4.356 2.62-4.385 8.816.029 6.185.484 8.549 4.385 8.816 3.6.245 11.626.246 15.23 0 3.897-.266 4.356-2.62 4.385-8.816-.029-6.185-.484-8.549-4.385-8.816zm-10.615 12.816v-8l8 3.993-8 4.007z" />
                    </svg>
                </div>
            </a>
            {{/if}}
            {{#if t.social.twitter}}
            <a href="https://twitter.com/{{t.social.twitter}}?gencookie=1" target="_blank">
                <div class="profile-header-social-item" title="Twitter">
                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                        <path
                            d="M24 4.557c-.883.392-1.832.656-2.828.775 1.017-.609 1.798-1.574 2.165-2.724-.951.564-2.005.974-3.127 1.195-.897-.957-2.178-1.555-3.594-1.555-3.179 0-5.515 2.966-4.797 6.045-4.091-.205-7.719-2.165-10.148-5.144-1.29 2.213-.669 5.108 1.523 6.574-.806-.026-1.566-.247-2.229-.616-.054 2.281 1.581 4.415 3.949 4.89-.693.188-1.452.232-2.224.084.626 1.956 2.444 3.379 4.6 3.419-2.07 1.623-4.678 2.348-7.29 2.04 2.179 1.397 4.768 2.212 7.548 2.212 9.142 0 14.307-7.721 13.995-14.646.962-.695 1.797-1.562 2.457-2.549z" />
                    </svg>
                </div>
            </a>
            {{/if}}
            {{/each}}
        </div>
        <div class="profile-header-info">
            <a href="#tab-1" class="tab">
                <div class="profile-header-info-item tab-active">
                    <p>General</p>
                    <div class="little-bar"></div>
                </div>
            </a>
            <a href="#tab-2" class="tab">
                <div class="profile-header-info-item">
                    <p>Animes Siguiendo</p>
                    <div class="little-bar"></div>
                </div>
            </a>
            <a href="#tab-3" class="tab">
                <div class="profile-header-info-item">
                    <p>Animes Gustado</p>
                    <div class="little-bar"></div>
                </div>
            </a>
        </div>
    </div>
    <div class="profile-middle-container">
        <div id="tab-1" class="profile-middle-info show" style="margin-top: 15px !important;">
            <div class="profile-middle-info-user">

                {{#each ../social as |t|}}
                {{#if t.about}}

                <div class="profile-middle-info-user-item">
                    <h5>Informacion</h5>
                    <p>{{t.about}}</p>
                </div>
                {{else}}
                <div class="profile-middle-info-user-item">
                    <h5>Informacion</h5>
                    <p>Este usuario no tiene descripci√≥n.</p>
                </div>
                {{/if}}
                {{/each}}
            </div>
            <div class="profile-middle-collections-container">
                <div class="profile-middle-collctions-title">
                    <p>Colecciones</p>
                </div>
                {{#each ../collectionspreview as |tw|}}
                {{tw.image}}
                {{/each}}
                {{#each ../collections as |col|}}
                <div class="profile-middle-collections-item" data-id="{{col.id}}">
                    <div class="profile-middle-collections-header">
                        <p>{{col.name}}</p>
                    </div>
                    <div class="profile-middle-collections-info">
                        <div class="profile-middle-collections-info-item">
                            <p>Total</p>
                            <span>{{col.toplam}}</span>
                        </div>
                        <div class="profile-middle-collections-info-item">
                            <p>Creacion</p>
                            <span>Hace {{#moment col.created}}{{/moment}}</span>
                        </div>
                        <!--<div class="profile-middle-collections-info-item">
                        <p>Popular</p>
                        <span>5</span>
                    </div>-->
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
        <div id="tab-2" class="profile-middle-info">
            <div class="Anime-box profile-anime-box-follow">
                {{#each ../follows as |follow|}}
                {{#ifEquals follow.type "serie"}}
                <a href="/anime/{{follow.id}}">
                    <div class="Anime-item">
                        <div class="shadow">
                            {{#if follow.imdb}}
                            <div class="Anime-tag-imdb">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                                    <path
                                        d="M12 .587l3.668 7.568 8.332 1.151-6.064 5.828 1.48 8.279-7.416-3.967-7.417 3.967 1.481-8.279-6.064-5.828 8.332-1.151z" />
                                </svg>
                                <p>{{follow.imdb}}</p>
                            </div>
                            {{/if}}
                            {{#if follow.animestatus}}
                            <div class="Anime-tag-animestatus">
                                <p>{{follow.animestatus}}</p>
                            </div>
                            {{/if}}
                            {{#if follow.quality}}
                            <div class="Anime-tag-quality">
                                <p>{{follow.quality}}</p>
                            </div>
                            {{/if}}
                            <div class="Anime-Play">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                                    <path d="M3 22v-20l18 10-18 10z" />
                                </svg>
                            </div>
                        </div>
                        <div class="Anime-img">
                            <img loading="lazy" src="https://animegenzai.xyz/public/upload/cover/{{follow.image}}">
                        </div>
                        <div class="Anime-title-container">
                            <div class="Anime-title Anime-H1">
                                <p>
                                    {{follow.title}}
                                </p>
                            </div>

                        </div>

                    </div>
                </a>
                {{/ifEquals}}
                {{#ifEquals follow.type "movie"}}
                <a href="/movie/{{follow.id}}">
                    <div class="Anime-item">
                        <div class="shadow">
                            {{#if follow.imdb}}
                            <div class="Anime-tag-imdb">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                                    <path
                                        d="M12 .587l3.668 7.568 8.332 1.151-6.064 5.828 1.48 8.279-7.416-3.967-7.417 3.967 1.481-8.279-6.064-5.828 8.332-1.151z" />
                                </svg>
                                <p>{{follow.imdb}}</p>
                            </div>
                            {{/if}}
                            {{#if follow.quality}}
                            <div class="Anime-tag-quality">
                                <p>{{follow.quality}}</p>
                            </div>
                            {{/if}}
                            <div class="Anime-Play">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                                    <path d="M3 22v-20l18 10-18 10z" />
                                </svg>
                            </div>
                        </div>
                        <div class="Anime-img">
                            <img loading="lazy" src="https://animegenzai.xyz/public/upload/cover/{{follow.image}}">
                        </div>
                        <div class="Anime-title-container">
                            <div class="Anime-title Anime-H1">
                                <p>
                                    {{follow.title}}
                                </p>
                            </div>

                        </div>

                    </div>
                </a>
                {{/ifEquals}}
                {{/each}}
            </div>
        </div>
        <div id="tab-3" class="profile-middle-info">
            <div class="Anime-box profile-anime-box-follow">
                {{#each ../followsanime as |follows|}}
                {{#ifEquals follows.type "serie"}}
